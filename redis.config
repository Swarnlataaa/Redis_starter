save 900 1           # Save every 15 minutes if at least 1 key has changed
save 300 10          # Save every 5 minutes if at least 10 keys have changed
save 60 10000        # Save every 1 minute if at least 10,000 keys have changed
dbfilename dump.rdb  # Specify the filename for the RDB snapshot
dir /path/to/directory # Specify the directory for RDB files
